{
  "version": "0.2.0",
  "configurations": [{
    "type": "chrome",
    "request": "launch",
    "name": "vuejs: chrome",
    "url": "http://localhost:4000",
    "webRoot": "${workspaceRoot}/build",
    "sourceMaps": true,
    "sourceMapPathOverrides": {
      "webpack:///*": "${webRoot}/client/components/*"
    }
  }]
}
